#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    int a;
    float b;
    char c;
    double d;
    
    scanf("%c", &c);
    scanf("%d", &a);
    scanf("%f", &b);
    scanf("%lf", &d);

    printf("%c \n",c);
    printf("%d \n",a);
    printf("%f \n",b);
    printf("%1f \n",d);

}

